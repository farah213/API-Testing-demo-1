		{
			"name": "Create Booking",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var JsonData = pm.response.json()\r",
							"pm.environment.set(\"Id\",JsonData.bookingid)\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"First name validation\", function () { \r",
							"    pm.expect(JsonData.booking.firstname).to.eql(\"Neela\");\r",
							"});\r",
							"pm.test(\"First name validation with negative value\", function () { \r",
							"    pm.expect(JsonData.booking.firstname).to.eql(\"neela\");\r",
							"});\r",
							"pm.test(\"Last name validation with negative value\", function () { \r",
							"    pm.expect(JsonData.booking.firstname).to.eql(\"islam\");\r",
							"});\r",
							"pm.test(\"Last name validation\", function () {\r",
							"    pm.expect(JsonData.booking.lastname).to.eql(\"Islam\");\r",
							"});\r",
							"pm.test(\"Total Price check with negative value\", function () {\r",
							"    pm.expect(JsonData.booking.totalprice).to.eql(900);\r",
							"});\r",
							"pm.test(\"Total Price check with postive value\", function () {\r",
							"    pm.expect(JsonData.booking.totalprice).to.eql(956);\r",
							"});\r",
							"pm.test(\"Checkin date with postive value\", function () {\r",
							"    pm.expect(JsonData.booking.bookingdates.checkin).to.eql(\"2022-07-24\");\r",
							"});\r",
							"pm.test(\"Checkin date with negative value\", function () {\r",
							"    pm.expect(JsonData.booking.bookingdates.checkin).to.eql(\"2022-07-27\");\r",
							"});\r",
							"pm.test(\"Checkout date with negative value\", function () {\r",
							"    pm.expect(JsonData.booking.bookingdates.checkout).to.eql(\"2022-09-25\");\r",
							"});\r",
							"pm.test(\"Checkout date  with postive value\", function () {\r",
							"    pm.expect(JsonData.booking.bookingdates.checkout).to.eql(\"2022-08-25\");\r",
							"});\r",
							"\r",
							""
						],
